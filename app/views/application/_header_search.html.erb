<li>
  <%= form_tag search_path, method: :get, class: "navbar-search pull-left" do %>
    <div class="input-append">
      <%= text_field_tag :q, nil, placeholder: "Search", data: { provide: "typeahead", source: mod_names } %>
      <%= button_tag icon_tag("search"), class: "btn" %>
    </div>
  <% end %>
</li>
